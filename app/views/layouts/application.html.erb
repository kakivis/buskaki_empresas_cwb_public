<!DOCTYPE html>
<html>
  <head>
    <title>Buskaki Empresas CWB</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

    <% flash.each do |key, value| %>
      <%= content_tag(:div, class: "alert alert-#{key} alert-dismissable") do %>
        <%= value %>
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <% end %>
    <% end %>
    <script src="//maps.google.com/maps/api/js?key=INSERT_YOUR_KEY_HERE"></script>
    <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
    <script src='//cdn.rawgit.com/printercu/google-maps-utility-library-v3-read-only/master/infobox/src/infobox_packed.js' type='text/javascript'></script>
    <script src='//cdn.jsdelivr.net/gmaps4rails/2.1.2/gmaps4rails.js'> </script>
    <script src='https://cdn.jsdelivr.net/npm/underscore@1.13.0/underscore-umd-min.js' type='text/javascript'></script>
  </head>

  <body>
  <div id="menus-div">
    <%= render 'layouts/header' %>
    <%= render "shared/menu" if should_render_menu %>
  </div>
  <hr>
    <%= yield %>
  </body>
</html>
